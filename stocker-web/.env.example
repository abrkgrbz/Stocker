# Application
VITE_APP_NAME=Stocker
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:5104
VITE_API_TIMEOUT=30000
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_TOKEN_KEY=stocker_token
VITE_AUTH_REFRESH_TOKEN_KEY=stocker_refresh_token
VITE_AUTH_TENANT_KEY=stocker_tenant

# Feature Flags
VITE_FEATURE_DARK_MODE=true
VITE_FEATURE_MULTI_LANGUAGE=true
VITE_FEATURE_NOTIFICATIONS=true
VITE_FEATURE_WEBSOCKET=false

# Third Party Services
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Storage
VITE_STORAGE_PREFIX=stocker_
VITE_MAX_FILE_SIZE=10485760

# Security
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=debug