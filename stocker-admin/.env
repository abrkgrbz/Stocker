# API Configuration
VITE_API_URL=https://api.stoocker.app
VITE_API_BASE_URL=https://api.stoocker.app
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=Stocker Admin
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Environment
VITE_NODE_ENV=development
VITE_ENVIRONMENT=development

# Auth Configuration
VITE_AUTH_TOKEN_KEY=stocker_admin_token
VITE_AUTH_REFRESH_TOKEN_KEY=stocker_admin_refresh_token

# Features
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_LOGGING=true
VITE_ENABLE_2FA=true
VITE_ENABLE_MOCK_AUTH=false

# Security
VITE_SECURE_COOKIES=true
VITE_SESSION_TIMEOUT=3600000
VITE_CSP_REPORT_URI=https://api.stoocker.app/csp-report
VITE_ENCRYPTION_KEY=stocker-admin-encryption-key-2025

# WebSocket/SignalR
VITE_WS_URL=wss://api.stoocker.app/hub
VITE_WS_RECONNECT_INTERVAL=5000

# Storage
VITE_STORAGE_TYPE=sessionStorage
VITE_STORAGE_PREFIX=stocker_

# Error Tracking (Sentry) - Optional
# VITE_SENTRY_DSN=
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAYS_SESSION_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0

# Analytics (Optional)
VITE_GA_TRACKING_ID=
VITE_GTM_ID=

# Debugging
VITE_DEBUG=false
VITE_LOG_LEVEL=warn