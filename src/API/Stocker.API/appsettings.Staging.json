{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "DatabaseSettings": {
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": true,
    "CommandTimeout": 45
  },
  "EmailSettings": {
    "EnableSsl": true,
    "Timeout": 20000
  },
  "StorageSettings": {
    "Provider": "AzureBlob",
    "MaxFileSizeInBytes": 26214400
  }
}