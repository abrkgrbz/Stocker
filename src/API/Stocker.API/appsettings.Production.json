{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "WILL_BE_OVERRIDDEN_BY_ENVIRONMENT_VARIABLE",
    "Issuer": "https://api.stoocker.app",
    "Audience": "https://stoocker.app",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 7
  },
  "PasswordPolicy": {
    "MinimumLength": 8,
    "MaximumLength": 128,
    "RequireUppercase": true,
    "RequireLowercase": true,
    "RequireDigit": true,
    "RequireNonAlphanumeric": true,
    "RequiredUniqueChars": 4,
    "PreventCommonPasswords": true,
    "PreventUserInfoInPassword": true,
    "PasswordHistoryCount": 5,
    "PasswordExpirationDays": 90,
    "MaxFailedAccessAttempts": 5,
    "LockoutDurationMinutes": 15,
    "MinimumStrengthScore": 3,
    "SpecialCharacters": "!@#$%^&*()_+-=[]{}|;:,.<>?"
  },
  "EmailSettings": {
    "SmtpHost": "mail.privateemail.com",
    "SmtpPort": 465,
    "SmtpUsername": "info@stoocker.app",
    "SmtpPassword": "WILL_BE_OVERRIDDEN_BY_ENVIRONMENT_VARIABLE",
    "EnableSsl": true,
    "FromEmail": "info@stoocker.app",
    "FromName": "Stoocker",
    "UseStartTls": false,
    "EnableEmail": true,
    "BaseUrl": "https://stoocker.app",
    "TemplatesPath": "EmailTemplates",
    "Timeout": 30000
  },
  "Hangfire": {
    "DashboardPath": "/hangfire",
    "ServerName": "Stocker-Production",
    "WorkerCount": 8,
    "Queues": ["critical", "default", "low"],
    "RetryAttempts": 3,
    "JobExpirationCheckInterval": "01:00:00",
    "CountersAggregateInterval": "00:05:00"
  },
  "DatabaseSettings": {
    "ServerName": "WILL_BE_OVERRIDDEN_BY_ENVIRONMENT_VARIABLE",
    "MasterDatabaseName": "StockerMasterDb",
    "UseWindowsAuthentication": false,
    "UserId": "WILL_BE_OVERRIDDEN_BY_ENVIRONMENT_VARIABLE",
    "Password": "WILL_BE_OVERRIDDEN_BY_ENVIRONMENT_VARIABLE",
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "CommandTimeout": 30,
    "MaxRetryCount": 3,
    "MaxRetryDelay": 30
  },
  "AdminCredentials": {
    "DefaultPassword": "WILL_BE_OVERRIDDEN_BY_ENVIRONMENT_VARIABLE",
    "RequirePasswordChangeOnFirstLogin": true
  }
}