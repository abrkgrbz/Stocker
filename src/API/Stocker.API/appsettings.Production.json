{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "DatabaseSettings": {
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "CommandTimeout": 30
  },
  "EmailSettings": {
    "EnableSsl": true,
    "Timeout": 30000
  },
  "StorageSettings": {
    "Provider": "AzureBlob",
    "MaxFileSizeInBytes": 52428800
  }
}