# Domain Configuration
DOMAIN=stoocker.app
ADMIN_URL=https://admin.stoocker.app
API_URL=https://api.stoocker.app

# Environment
ENVIRONMENT=Production
MOCK_ENABLED=false

# Database
DB_CONNECTION_STRING=Host=postgres;Database=stocker;Username=postgres;Password=your_password_here

# Redis
REDIS_CONNECTION_STRING=redis:6379

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_here_min_32_chars
JWT_ISSUER=https://api.stoocker.app
JWT_AUDIENCE=https://admin.stoocker.app

# API Image
API_IMAGE=stocker-api:latest