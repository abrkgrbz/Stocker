{
  "expo": {
    "name": "Stocker Mobile",
    "slug": "stocker-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "stocker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stocker.mobile"
    },
    "android": {
      "package": "com.stocker.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
