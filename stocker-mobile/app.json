{
  "expo": {
    "name": "Stocker Mobile",
    "slug": "stocker-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "stocker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stocker.mobile",
      "associatedDomains": [
        "applinks:stocker.app",
        "applinks:app.stocker.com"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["stocker", "stockerapp"]
          }
        ]
      }
    },
    "android": {
      "package": "com.stocker.mobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "stocker.app",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "app.stocker.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "stocker"
            },
            {
              "scheme": "stockerapp"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store",
      "expo-local-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
