# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=stocker_master
DB_USER=postgres
DB_PASSWORD=your_secure_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# JWT
JWT_SECRET_KEY=your_very_secure_secret_key_min_32_characters
JWT_ISSUER=stocker
JWT_AUDIENCE=stocker

# API URLs
API_URL=http://localhost:5104
SIGNALR_URL=http://localhost:5104/hubs

# Environment
ENVIRONMENT=development

# Email (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# Coolify Webhook (for CI/CD)
COOLIFY_WEBHOOK_URL=https://your-coolify-instance/webhook/your-app-id

# Test Environment URLs
TEST_API_URL=https://api.test.stocker.app
TEST_WEB_URL=https://test.stocker.app